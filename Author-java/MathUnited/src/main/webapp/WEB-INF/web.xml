<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <display-name>MathUnited</display-name>
    <servlet>
        <servlet-name>view</servlet-name>
        <servlet-class>nl.math4all.mathunited.ViewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>edit</servlet-name>
        <servlet-class>nl.math4all.mathunited.editor.EditServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>refresh-lock</servlet-name>
        <servlet-class>nl.math4all.mathunited.editor.RefreshLockServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>process-item</servlet-name>
        <servlet-class>nl.math4all.mathunited.editor.ProcessItemServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>postcontent</servlet-name>
        <servlet-class>nl.math4all.mathunited.editor.PostContentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reinit</servlet-name>
        <servlet-class>nl.math4all.mathunited.InitServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>nl.math4all.mathunited.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>nl.math4all.mathunited.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginstate</servlet-name>
        <servlet-class>nl.math4all.mathunited.LoginStateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>forgotpassword</servlet-name>
        <servlet-class>nl.math4all.mathunited.LoginForgotPasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>nl.math4all.mathunited.LoginRegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>setrepo</servlet-name>
        <servlet-class>nl.math4all.mathunited.SetRepoServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>encodeBase64</servlet-name>
        <servlet-class>nl.math4all.mathunited.GeogebraGenerator</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>view</servlet-name>
        <url-pattern>/view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>edit</servlet-name>
        <url-pattern>/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>refresh-lock</servlet-name>
        <url-pattern>/refresh-lock</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>process-item</servlet-name>
        <url-pattern>/processitem</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>postcontent</servlet-name>
        <url-pattern>/postcontent</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reinit</servlet-name>
        <url-pattern>/reinit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginstate</servlet-name>
        <url-pattern>/loginstate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>forgotpassword</servlet-name>
        <url-pattern>/forgotpassword</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>setrepo</servlet-name>
        <url-pattern>/setrepo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>encodeBase64</servlet-name>
        <url-pattern>/geogebra</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
